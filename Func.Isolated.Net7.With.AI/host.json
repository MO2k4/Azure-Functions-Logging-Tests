{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        },
        "logLevel": {
          "Microsoft": "Warning",
          "Azure.Core": "Error",
          "Host": "Warning",
          "default": "Information"
        }
    }
}