{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        },
        "logLevel": {
          "Microsoft": "Warning",
          "Azure.Core": "Error",
          "Host.Results": "Information",
          "Host.Executor": "Warning",
          "Host.General": "Warning",
          "Host.Controllers.Admin": "Warning",
          "Host.Aggregator": "Information",
          "default": "Information"
        }
    }
}